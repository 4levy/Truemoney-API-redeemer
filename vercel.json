{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/redeem",
      "methods": ["POST"],
      "dest": "api/index.js"
    },
    {
      "src": "/api",
      "methods": ["GET"],
      "dest": "api/index.js"
    }
  ]
}
